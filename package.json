{
	"name": "api_nova_james",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"dev": "ts-node-dev --respawn --transpile-only ./src/server.ts",
		"typeorm": "typeorm-ts-node-commomjs",
		"Ecreate": "npx typeorm entity:create",
		"Mcreate": "npx typeorm migration:create",
		"Mgenerate": "npm run typeorm migration:generate -- -d src/database/typeorm.ts",
		"Mrun": "npm run typeorm migration:run -- -d src/database/typeorm.ts",
		"Mrevert": "npm run typeorm migration:revert -- -d src/database/typeorm.ts",
		"Mshow": "npm run typeorm migration:show -- -d src/database/typeorm.ts",
		"build": "npm i --omit=dev && npx tsc"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"pg": "^8.11.0",
		"reflect-metadata": "^0.1.13",
		"ts-node": "^10.9.1",
		"typeorm": "^0.3.16",
		"typescript": "^5.0.4",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.17",
		"@types/node": "^18.16.13",
		"@types/pg": "^8.6.6",
		"@types/uuid": "^9.0.1",
		"ts-node-dev": "^2.0.0"
	}
